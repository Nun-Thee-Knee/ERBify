{
  "name": "erbify",
  "displayName": "erbify",
  "description": "Converts haml file to erb",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nun-Thee-Knee/erbify"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "erbify.convertHamlToErb",
        "title": "Erbify"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Erbify Settings",
      "properties": {
        "erbify.apiKey": {
          "type": "string",
          "default": "",
          "description": "Your API key"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@google/generative-ai": "^0.14.1",
    "@langchain/google-genai": "^0.0.21",
    "@vscode/vsce": "^2.29.0"
  }
}
